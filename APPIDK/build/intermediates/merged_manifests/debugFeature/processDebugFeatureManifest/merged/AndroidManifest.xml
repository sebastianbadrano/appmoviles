<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    featureSplit="APPIDK"
    package="com.example.macbookpro.appidk.app"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="25"
        android:targetSdkVersion="28" /> <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:debuggable="true"
        android:testOnly="true" >
        <activity
            android:name="com.example.macbookpro.appidk.APPIDK.Home"
            android:label="@string/app_name"
            android:splitName="APPIDK" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.macbookpro.appidk.APPIDK.registro"
            android:label="@string/title_activity_registro"
            android:splitName="APPIDK" />
        <activity
            android:name="com.example.macbookpro.appidk.APPIDK.Perfil"
            android:splitName="APPIDK" />
    </application>

</manifest>